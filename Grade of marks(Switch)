package Day3;

public class Switch {
	public static void main(String[] args)
	{
		char Grade='A';
		switch(Grade)
		{
		case'A':System.out.println("Dist");
		break;
		case'B':System.out.println("1st class");
		break;
		case'C':System.out.println("2nd class");
		break;
		case'F':System.out.println("Fail");
		break;
		
	}
}
}
